<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE suite SYSTEM "https://testng.org/testng-1.0.dtd">
<suite name="Open Cart Regression Test" thread-count="10" parallel="tests">

<listeners>
		<listener class-name="com.qa.opencart.listeners.ExtentReportListener" />
		<listener class-name="com.qa.opencart.listeners.TestAllureListener" />
		<listener class-name="com.qa.opencart.listeners.AnnotationTransformer" />
	</listeners>
  <test  name="Login Test">
    <classes>
      <class name="com.qa.opencart.tests.LoginTest"/>
    </classes>
  </test> <!-- Test -->
  
  <test  name="Accounts Test">
    <classes>
      <class name="com.qa.opencart.tests.AccountPageTest"/>
    </classes>
  </test> <!-- Test -->
  
    <test  name="ProductPage Test">
    <classes>
      <class name="com.qa.opencart.tests.ProductPageTest"/>
    </classes>
  </test> <!-- Test -->
  
   <test  name="Register Test">
    <classes>
      <class name="com.qa.opencart.tests.RegisterTest"/>
    </classes>
  </test> <!-- Test -->
</suite> <!-- Suite -->
