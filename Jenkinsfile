    agent any

    stages {
        stage('Build') {
            steps {
                echo 'build it'
            }
        }
        
        stage('Test') {
            steps {
                echo 'test it'
            }
        }
        
        stage('Merge') {
            steps {
                echo 'merge it'
            }
        }
        
        stage('Deploy') {
            steps {
                echo 'deploy it'
            }
        }
    }
}
